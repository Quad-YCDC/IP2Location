CREATE TABLE IF NOT EXISTS IP2Location_data (
  IP_FROM NUMERIC(39) NOT NULL,
  IP_TO NUMERIC(39) NOT NULL,
  COUNTRY_CODE CHAR(2) NOT NULL,
  COUNTRY_NAME VARCHAR(64) NOT NULL,
  REGION VARCHAR(128) NOT NULL,
  CITY VARCHAR(128) NOT NULL,
  LATITUDE NUMERIC(10,6) NOT NULL,
  LONGITUDE NUMERIC(10,6) NOT NULL,
  ISP_NAME VARCHAR(256) NOT NULL,
  PRIMARY KEY (id)
);

COMMENT ON TABLE IP2Location_data IS 'IP2LOCATION 정보';
COMMENT ON COLUMN IP2Location_data.IP_FROM IS 'IP 주소 범위 시작';
COMMENT ON COLUMN IP2Location_data.IP_TO IS 'IP 주소 범위 마지막';
COMMENT ON COLUMN IP2Location_data.COUNTRY_CODE IS '2자리 나라명';
COMMENT ON COLUMN IP2Location_data.COUNTRY_NAME IS '나라명';
COMMENT ON COLUMN IP2Location_data.REGION IS '지역 이름';
COMMENT ON COLUMN IP2Location_data.CITY IS '도시이름';
COMMENT ON COLUMN IP2Location_data.LATITUDE IS '도시 위도';
COMMENT ON COLUMN IP2Location_data.LONGITUDE IS '도시 경도';
COMMENT ON COLUMN IP2Location_data.ISP_NAME IS '인터넷 서비스 제공업체';


CREATE TABLE IF NOT EXISTS IP2LOCATION_Result (
ipcomputation NUMERIC(39) NOT NULL,
Lati NUMERIC(10,6) NOT NULL,
Long NUMERIC(10,6) NOT NULL,
PRIMARY KEY (id)
);

COMMENT ON TABLE IP2LOCATION_Result IS 'Ti 정보 위치';
COMMENT ON COLUMN IP2LOCATION_Result.ipcomputation  'ip연산 값';
COMMENT ON COLUMN IP2LOCATION_Result.Lati ' 도시 위도';
COMMENT ON COLUMN IP2LOCATION_Result.Long ' 도시 경도';